@model string

<div class="form-group">
    @if (ViewData["LabelName"] == null)
    {
        @Html.LabelFor(model => model, new { @class = "col-lg-2 control-label" })
    }
    else
    {
        @Html.Label(ViewData["LabelName"].ToString(), new { @class = "col-lg-2 control-label" })
    }

    <div class="col-lg-10">
        @Html.TextBoxFor(model => model, new { @class = "form-control" })
        @Html.ValidationMessageFor(m => m)
    </div>
</div>
